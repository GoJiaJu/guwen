/* pages/components/uploadCard/index.wxss */
.upload-card{
  background: #fff;
  border-radius: 5rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 30rpx 0;
}
.upload-card text{color: #999;}
.upload-card text text{color: #438cfe;}
.upload-card>view{
  width: 80%;
  height: auto;
  margin-bottom: 20rpx;
  position: relative;
}
.upload-card>view image{
  width: 100%;
  display: flex;
}
.upload-btn{
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  background: rgba(0,0,0,.7);
  border-radius: 50%;
  top:50%;
  left: 50%;
  transform: translateX(-50%) translateY(-50%);
  z-index: 5;
  background-size: 50rpx auto;
  background-position: center center;
  background-repeat: no-repeat;
  background-image: url(data:image/png;base64,);
}
