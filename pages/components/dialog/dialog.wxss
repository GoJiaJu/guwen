/* pages/components/dialog/dialog.wxss */
.dialog{
   width: 100%;
   height: 100%;
   background: rgba(0,0,0,0.5);
   position: fixed;
   top:0;
   left: 0;
}
.dialog>view{
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    transform: translateY(-10px);
}