.add-block{
  background: #f2f2f2;
  padding: 40rpx;
  box-sizing: border-box;
}
.add-block>view{
  width: 90%;
  box-sizing: border-box;
  padding: 40rpx 0;
  background: #fff;
  border-radius: 20rpx;
  margin: 0 auto;
  box-shadow: 0 0 10rpx 2rpx #ddd;
  flex-direction: column;
  color: #ccc;
}
.add-block>view text:nth-child(2){
  margin: 40rpx 0 10rpx 0;
  font-size: 36rpx;
  color: #666;
}
.add-block-plus{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background-size: 60% auto;
  background-repeat: no-repeat;
  background-position: center center;
  background-image: url(data:image/png;base64,);
  background-color: #438cfe;
}
.add-tips{
  box-sizing: border-box;
  padding: 20rpx;
  color: #ccc;
}
.add-tips view{line-height: 50rpx;}
.add-tips view:nth-child(1){color: #438cfe;}
.add-tips view text{color: #999;}